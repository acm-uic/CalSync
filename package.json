{
  "name": "calsync",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Bharat Middha <5100938+bmiddha@users.noreply.github.com>",
  "repository": "https://github.com/acm-uic/calsync.git",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "dev": "nodemon --exec ts-node src/index.ts",
    "format": "prettier --list-different src/**/*.ts *.js *.json",
    "format:fix": "prettier --write src/**/*.ts *.js *.json",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "yarn lint --fix",
    "prepare": "husky install",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "0.27.2",
    "dotenv": "^16.0.1",
    "winston": "3.8.1"
  },
  "devDependencies": {
    "@tsconfig/node18-strictest": "1.0.0",
    "@types/gapi.client.calendar": "3.0.10",
    "@types/node": "18.0.1",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "8.0.1",
    "nodemon": "2.0.19",
    "prettier": "2.7.1",
    "ts-node": "10.8.2",
    "typescript": "4.7.4"
  }
}
